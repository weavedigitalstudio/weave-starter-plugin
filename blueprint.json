{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"landingPage": "/wp-admin/admin.php?page=weave-starter-plugin",
	"preferredVersions": {
		"php": "8.2",
		"wp": "latest"
	},
	"features": {
		"networking": true
	},
	"steps": [
		{
			"step": "defineWpConfigConsts",
			"consts": {
				"WP_DEBUG": true,
				"WP_DEBUG_LOG": true
			}
		},
		{
			"step": "setSiteOptions",
			"options": {
				"blogname": "Weave Starter Plugin Demo",
				"timezone_string": "Pacific/Auckland",
				"permalink_structure": "/%postname%/"
			}
		},
		{
			"step": "login",
			"username": "admin",
			"password": "password"
		}
	]
}
