{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "weave-starter/starter-item-frontend",
	"title": "Starter Items Display",
	"category": "widgets",
	"icon": "portfolio",
	"description": "Display starter items with category filtering using the Interactivity API.",
	"textdomain": "weave-starter-plugin",
	"supports": {
		"html": false,
		"interactivity": true
	},
	"attributes": {
		"limit": {
			"type": "number",
			"default": 12
		},
		"columns": {
			"type": "number",
			"default": 3
		}
	},
	"editorScript": "file:./index.js",
	"viewScriptModule": "file:./view.js",
	"render": "file:./render.php",
	"style": "file:./style.css"
}
